    "short_description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "text",
      "required": true
    },
    "ean": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "string"
    },
    "material": {
      "type": "string"
    },
    "product_image2": {
      "type": "text"
    },
    "product_image3": {
      "type": "text"
    },
    "product_image4": {
      "type": "text"
    },
    "product_image5": {
      "type": "text"
    },
    "shops": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::shop.shop",
      "mappedBy": "products"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "departments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::department.department",
