    "sale_price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "product_url": {
      "type": "text",
      "required": true
    },
    "delivery": {
      "type": "string",
      "required": false
    },
    "width": {
      "type": "string",
      "required": false
    },
    "height": {
      "type": "string",
      "required": false
    },
    "depth": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "products"
